; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Goblin Simulator"
config/tags=PackedStringArray("template")
run/main_scene="res://scenes/game_root.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"
boot_splash/minimum_display_time.release=2000

[audio]

buses/default_bus_layout="res://audio_bus_layout.tres"

[autoload]

Globals="*res://autoload/globals/globals.gd"
SceneGirl="*res://autoload/scene_girl/scene_girl.gd"
MusicMan="*res://autoload/music_man.gd"
TestCamera="*res://addons/test_camera/test_camera_manager.gd"
BeehaveGlobalMetrics="*res://addons/beehave/metrics/beehave_global_metrics.gd"
BeehaveGlobalDebugger="*res://addons/beehave/debug/global_debugger.gd"
Fullscreener="res://autoload/fullscreener.gd"
Debug="res://autoload/debug.gd"

[display]

window/size/viewport_width=568
window/size/viewport_height=320
window/size/window_width_override=1136
window/size/window_height_override=640
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"

[editor_plugins]

enabled=PackedStringArray("res://addons/beehave/plugin.cfg", "res://addons/cider_wiki/plugin.cfg", "res://addons/palette_sprite_importer/plugin.cfg", "res://addons/smooth_pixel_subviewport_container/plugin.cfg", "res://addons/test_camera/plugin.cfg")

[global_group]

Projectile=""
Unit=""
Building=""
Castle=""

[gui]

theme/custom="res://gui_theme.tres"

[importer_defaults]

texture={
"detect_3d/compress_to": 0
}

